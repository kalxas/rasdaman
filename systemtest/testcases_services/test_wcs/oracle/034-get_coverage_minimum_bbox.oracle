<?xml version="1.0" encoding="UTF-8"?>
<gmlcov:RectifiedGridCoverage id="test_eobstest"
    xmlns="http://www.opengis.net/gml/3.2"
    
    
    
    
     >
    <boundedBy>
        <Envelope axisLabels="Long" srsDimension="1"
            srsName="/crs/EPSG/0/4326" uomLabels="degree">
            <lowerCorner>70</lowerCorner>
            <upperCorner>70.5</upperCorner>
        </Envelope>
    </boundedBy>
    <domainSet>
        <RectifiedGrid dimension="1" id="test_eobstest-grid">
            <limits>
                <GridEnvelope>
                    <low>221</low>
                    <high>221</high>
                </GridEnvelope>
            </limits>
            <axisLabels>Long</axisLabels>
            <origin>
                <Point id="test_eobstest-origin" srsName="/crs/EPSG/0/4326">
                    <pos>70.25</pos>
                </Point>
            </origin>
            <offsetVector srsName="/crs/EPSG/0/4326">0.5</offsetVector>
        </RectifiedGrid>
    </domainSet>
    <rangeSet>
        <DataBlock>
            <rangeParameters/>
            <tupleList cs=" " ts=",">-9999</tupleList>
        </DataBlock>
    </rangeSet>
    <coverageFunction>
        <GridFunction>
            <sequenceRule axisOrder="+3">Linear</sequenceRule>
            <startPoint>221</startPoint>
        </GridFunction>
    </coverageFunction>
    <gmlcov:rangeType>
        <swe:DataRecord>
            <swe:field name="tg">
                <swe:Quantity >
                    <swe:label>tg</swe:label>
                    <swe:description>Count of the number of observations from the MERIS sensor contributing to this bin cell</swe:description>
                    <swe:nilValues>
                        <swe:NilValues>
                            <swe:nilValue reason="The nil value represents an error in the sensor.">-9999</swe:nilValue>
                        </swe:NilValues>
                    </swe:nilValues>
                    <swe:uom code="10^0"/>
                </swe:Quantity>
            </swe:field>
        </swe:DataRecord>
    </gmlcov:rangeType>
    <gmlcov:metadata>
        <gmlcov:Extension>
            <covMetadata>
                <bands>
                    <tg>
                        <units>Celsius</units>
                        <long_name>mean temperature</long_name>
                        <standard_name>air_temperature</standard_name>
                        <scale_factor>0.01</scale_factor>
                        <_FillValue>-9999</_FillValue>
                    </tg>
                </bands>
                <Creator>This is a test creator file</Creator>
                <Project>This is another test file</Project>
                <Title>This is a test file</Title>
            </covMetadata>
        </gmlcov:Extension>
    </gmlcov:metadata>
