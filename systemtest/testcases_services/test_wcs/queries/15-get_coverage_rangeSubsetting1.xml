<?xml version="1.0" encoding="UTF-8"?>
<wcs:GetCoverage
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wcs="http://www.opengis.net/wcs/2.0"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:rsub="http://www.opengis.net/wcs/range-subsetting/1.0"
    xsi:schemaLocation="http://schemas.opengis.net/wcs/2.0 ../wcsAll.xsd"
    service="WCS" version="2.0.1">
  <wcs:CoverageId>test_rgb</wcs:CoverageId>
  <wcs:Extension>
    <rsub:rangeSubset>
      <rsub:rangeItem>
        <rsub:rangeInterval>
          <rsub:startComponent>red</rsub:startComponent>
          <rsub:endComponent>green</rsub:endComponent>
        </rsub:rangeInterval>
      </rsub:rangeItem>
    </rsub:rangeSubset>
  </wcs:Extension>
</wcs:GetCoverage>
