{
    "config": {
    "service_url": "PETASCOPE_URL",
    "tmp_directory": "/tmp/",
    "crs_resolver": "SECORE_URL",
    "default_crs": "SECORE_URL/crs/EPSG/0/4326",
    "mock": false,
    "automated": true,
    "track_files": false,
    "default_null_values": [ 
         "155"
       ]
    },
    "input": {
        "coverage_id": "test_time3d", 
        "paths": [
            "*.tif"
        ]
    },
    "recipe": {
        "name": "time_series_regular",
        "options": {           
	      "time_start": "1950-01-01T00:00:00",
	      "time_format": "auto",
	      "time_crs": "http://localhost:8080/def/crs/OGC/0/AnsiDate?axis-label=\"time\"",
	      "time_step": "1 days",
	      "tiling": "ALIGNED [0:100, 0:100, 0:20]" 
        }
    }
}
