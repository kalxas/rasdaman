{
    "config": {
        "service_url": "PETASCOPE_URL",
        "tmp_directory": "/tmp/",
        "crs_resolver": "SECORE_URL/",
        "default_crs": "SECORE_URL/crs/EPSG/0/4326",
        "mock": false,
        "automated": true,
        "subset_correction": false,
	"track_files": false
    },
    "input": {
        "coverage_id": "test_fractional_time_unixtime", 
        "paths": [
            "*.tif"
        ]
    },
    "recipe": {
        "name": "time_series_irregular", 
        "options": {
            "time_parameter": { 
                "filename": {
        	  "regex": "(.*)_\\[(.+?)\\]\\.(.*)", 
        	  "group": "2" 
	        },
		"datetime_format": "YYYY-MM-DDTHH:mm:ss.SSS" 
            },
	   "time_crs": "SECORE_URL/crs/OGC/0/UnixTime",
	   "band_names": [
	       "b1",
               "b2"
	    ]
        },
     
        "tiling": "ALIGNED [0:10, 0:1000, 0:500]"
    }
}
