{

  "config": {

    "service_url": "PETASCOPE_URL",

    "tmp_directory": "/tmp/",

    "track_files": false,

    "mock": false,

    "automated": true

  },



  "input": {

    "coverage_id": "test_wcps_grid_origin_negative_x_axis",

    "paths": [
      "*.tiff" 
    ]

  },



  "recipe": {

    "name": "time_series_irregular",

    "options": {


      "tiling": "ALIGNED [0:500, 0:500]",

      "time_parameter": {

        "filename": {

          "regex": "(.*)_(.*)_(.*)_(.*)_(.*)\\.(.*)",

          "group": "3"

        },

        "datetime_format": "YYYYMMDDTHHmmss"

      },

      "time_crs": "SECORE_URL/crs/OGC/0/AnsiDate"

    }

  }

}
